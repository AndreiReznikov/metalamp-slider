{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "esnext"],
    "allowJs": true,
    "checkJs": false,
    "sourceMap": true,
    "baseUrl": "./",
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "paths": {
      "~/*": ["src/*"]
    }
  },
}
